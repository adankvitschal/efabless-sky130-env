#----------------------------------------
# Tool Aliases
#----------------------------------------
alias magic='magic -d XR -rcfile $PDK_ROOT/$PDK/libs.tech/magic/$PDK.magicrc'
alias mmagic-$PDK='MAGTYPE=mag magic -rcfile $PDK_ROOT/$PDK/libs.tech/magic/$PDK.magicrc'
alias lmagic-$PDK='MAGTYPE=maglef magic -rcfile $PDK_ROOT/$PDK/libs.tech/magic/$PDK.magicrc'
alias xschem='xschem --rcfile $PDK_ROOT/$PDK/libs.tech/xschem/xschemrc'